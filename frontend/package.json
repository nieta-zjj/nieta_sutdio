{
  "name": "next-app-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:ignore-errors": "set NEXT_IGNORE_TYPE_ERROR=1 && next build",
    "export": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx -c eslint.config.mjs --ignore-pattern 'example/**'",
    "lint:check": "eslint . --ext .ts,.tsx -c eslint.config.mjs --ignore-pattern 'example/**'",
    "lint:strict": "eslint . --ext .ts,.tsx -c eslint.config.mjs --max-warnings=0 --ignore-pattern 'example/**'",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .prettierignore",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .prettierignore",
    "type-check": "tsc --noEmit",
    "check": "node scripts/code-quality.js",
    "check:eslint": "node scripts/code-quality.js --only=eslint",
    "check:types": "node scripts/code-quality.js --only=types",
    "check:deps": "node scripts/code-quality.js --only=deps",
    "check:format": "node scripts/code-quality.js --only=format",
    "report": "node scripts/generate-report.js",
    "report:eslint": "node scripts/generate-report.js --type=eslint",
    "report:complexity": "node scripts/generate-report.js --type=complexity",
    "report:deps": "node scripts/generate-report.js --type=deps",
    "report:format": "node scripts/generate-report.js --type=format",
    "clean": "rimraf .next out",
    "serve-static": "npx serve out"
  },
  "dependencies": {
    "@heroui/button": "2.2.19",
    "@heroui/code": "2.2.14",
    "@heroui/input": "2.4.19",
    "@heroui/kbd": "2.2.15",
    "@heroui/link": "2.2.16",
    "@heroui/listbox": "2.3.18",
    "@heroui/navbar": "2.2.17",
    "@heroui/react": "^2.7.8",
    "@heroui/snippet": "2.2.20",
    "@heroui/switch": "2.2.17",
    "@heroui/system": "2.4.15",
    "@heroui/theme": "2.4.15",
    "@iconify/react": "^4.1.1",
    "@internationalized/date": "^3.8.1",
    "@react-aria/i18n": "^3.12.9",
    "@react-aria/ssr": "3.9.8",
    "@react-aria/visually-hidden": "3.8.22",
    "clsx": "2.1.1",
    "framer-motion": "11.13.1",
    "intl-messageformat": "10.7.16",
    "next": "15.3.1",
    "next-themes": "0.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sonner": "^2.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.8",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "@next/eslint-plugin-next": "15.3.1",
    "@react-types/shared": "3.29.0",
    "@types/node": "22.15.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.25.1",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "16.0.0",
    "npm-check": "^6.0.1",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "rimraf": "^5.0.5",
    "tailwind-variants": "0.3.0",
    "tailwindcss": "3.4.16",
    "typescript": "5.6.3"
  }
}
